(
    {s, q3, q7, q8, f1, f2, f3, f4, f5, f6, f7, f8},
    {' ', 'a', '0', ':', '=', '+', '*', '<', '>', '='},
    {
        s' '    -> s,
        s'a'    -> f1,
        s'0'    -> f2,
        s':'    -> q3,
        q3'='   -> f3,
        s'+'    -> f4,
        s'*'    -> f5,
        s'<'    -> f6,
        s'<'    -> q7,
        s'<'    -> q8,
        q7'='   -> f7,
        q8'>'   -> f8
    },
    s,
    {f6, f7, f8}
)